#include <stdio.h>
#include <stdlib.h>



int multiplicar(int a, int b){

int result = a * b;

return result;
}






int main()
{
 int n1, n2, resultado;

 printf("Informe dois numeros: \n");
 scanf("%d %d", &n1,&n2);

 resultado = multiplicar(n1,n2);
    printf("Resultado da multiplicacao: %d\n", resultado);
}
